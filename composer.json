{
    "name": "ferdiebergado/phpnoframework",
    "description": "A web application skeleton using php without a framework",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "nikic/fast-route": "^1.3",
        "level-2/dice": "^3.0",
        "filp/whoops": "^2.2",
        "phpmailer/phpmailer": "^6.0",
        "bernard/bernard": "^0.12.4"
    },
    "require-dev": {
        "maximebf/debugbar": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        },
        "files": ["core/helpers.php"]
    },
